Include other_config
Host *
    # ControlMaster auto
    # ControlPath ~/.ssh/master-%r@%h:%p
    # ControlPersist 2m
    ServerAliveCountMax 10
    ServerAliveInterval 15
    SendEnv INSIDE_EMACS
    # IdentityFile ~/.ssh/id_rsa

